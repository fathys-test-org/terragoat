{
  "violation-policy": [
    {
      "class": "stored-secrets",
      "rules": {
        "aws-access-key-id": "alert",
        "basic-auth-credentials": "block"
      }
    },
    {
      "class": "cloud-misconfiguration",
      "rules": {
        "aws-classic-resource-usage": "alert",
        "s3-disabled-server-logging": "alert"
      }
    }
  ]
}